{
  "name": "kevincorbett",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:css": "stylelint \"**/*.{css,scss}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,md,mdx,json}\"",
    "reset": "rm -rf node_modules .next .eslintcache .cache package-lock.json pnpm-lock.yaml && pnpm install",
    "nvmrc": "echo $(node -p -e 'require(\"./package\").engines.node.split(\">=\").join(\"\")') > .nvmrc"
  },
  "dependencies": {
    "@mantine/colors-generator": "7.17.0",
    "@mantine/core": "7.17.0",
    "@mantine/hooks": "7.17.0",
    "@tabler/icons-react": "3.30.0",
    "@vercel/analytics": "1.5.0",
    "@vercel/speed-insights": "1.2.0",
    "clsx": "2.1.1",
    "gray-matter": "4.0.3",
    "lucide-react": "0.477.0",
    "next": "16.0.7",
    "next-mdx-remote": "5.0.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "tailwind-merge": "3.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.0",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/postcss": "4.0.9",
    "@tailwindcss/typography": "0.5.16",
    "@types/node": "22.13.5",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "eslint": "9.21.0",
    "eslint-config-next": "16.0.7",
    "eslint-config-prettier": "10.0.2",
    "eslint-plugin-jsonc": "2.19.1",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-prettier": "5.2.3",
    "husky": "9.1.7",
    "jsonc-eslint-parser": "2.4.0",
    "lint-staged": "15.4.3",
    "postcss": "8.5.3",
    "prettier": "3.5.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "stylelint": "16.15.0",
    "stylelint-config-standard": "37.0.0",
    "stylelint-config-tailwindcss": "1.0.0",
    "stylelint-order": "6.0.4",
    "tailwindcss": "4.0.9",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=22.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@vercel/speed-insights",
      "sharp"
    ]
  }
}
